    <script type="text/javascript">
    (function ($) {
      $(document).ready(function () {

        $('input[type="file"]').change(function(){
            var f = this.files[0];
            var name = f.name;
            $(this).next('.upload-path').text(name);
        });
      });
    })(jQuery);

    </script>
